{
  "expo": {
    "name": "Couple Bliss",
    "slug": "couple-bliss",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "couplebliss",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.couplebliss.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "Per scattare foto da condividere con il tuo partner",
        "NSPhotoLibraryUsageDescription": "Per scegliere foto da condividere",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.couplebliss.app",
      "versionCode": 3,
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "resizeMode": "cover",
          "backgroundColor": "#1a1a2e"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ff6b8a"
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bc459fd4-465a-4e71-b7f4-fcb2638b3123"
      },
      "router": {}
    },
    "owner": "knivo"
  }
}
